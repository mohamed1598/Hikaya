
@{
    ViewBag.Title = "Create";
}

@model hikaya.Models.Story

<form method="post">
    <div class="row">
        <div class="col-3">@Html.LabelFor(n => n.title)</div>
        <div class="col-3">@Html.EditorFor(n => n.title)</div>
    </div>
    <div class="row">
        <div class="col-3">@Html.LabelFor(n => n.tags)</div>
        <div class="col-3">@Html.EditorFor(n => n.tags)</div>
    </div>
    <div class="row">
        <div class="col-3"></div>
        <div class="col-3"><input type="submit" value="create" /></div>
    </div>
</form>

